# Server Configuration
PORT=3005
NODE_ENV=production

# Database Configuration
DB_DATABASE=your_database_name
DB_USERNAME=your_database_username
DB_PASSWORD=your_database_password
DB_HOST=localhost
DB_PORT=5432
DB_DIALECT=postgres

# Stripe Configuration
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_SECRET_KEY_TEST=your_stripe_test_key
STRIPE_SECRET_KEY_LIVE=your_stripe_live_key

# Authentication Configuration (REQUIRED)
# AUTH_KEY is used for JWT token verification - This is the PRIMARY key used by the system
AUTH_KEY=your_auth_key_here

# JWT_SECRET is kept for backward compatibility but NOT actively used
# Use AUTH_KEY instead
JWT_SECRET=your_jwt_secret_key

# Encryption Keys (if needed)
ENCRYPTION_KEY=your_encryption_key

